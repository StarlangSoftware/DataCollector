<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>NlpToolkit</groupId>
    <artifactId>DataCollector</artifactId>
    <version>1.0.6</version>
    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
    </properties>
    <repositories>
        <repository>
            <id>NlpToolkit</id>
            <url>http://haydut.isikun.edu.tr:8081/artifactory/NlpToolkit</url>
        </repository>
    </repositories>
    <dependencies>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>ParseTree</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>AnnotatedSentence</artifactId>
            <version>1.0.2</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>AnnotatedTree</artifactId>
            <version>1.0.1</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>Classification</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>DataGenerator</artifactId>
            <version>1.0.1</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>DependencyParser</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>Translation</artifactId>
            <version>1.0.1</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>MorphologicalAnalysis</artifactId>
            <version>1.0.3</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>Dictionary</artifactId>
            <version>1.0.5</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>NamedEntityRecognition</artifactId>
            <version>1.0.1</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>Util</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>PropBank</artifactId>
            <version>1.0.2</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>Corpus</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>WordNet</artifactId>
            <version>1.0.15</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>MorphologicalDisambiguation</artifactId>
            <version>1.0.2</version>
        </dependency>
        <dependency>
            <groupId>NlpToolkit</groupId>
            <artifactId>DataStructure</artifactId>
            <version>1.0.0</version>
        </dependency>
    </dependencies>

    <distributionManagement>
        <repository>
            <id>NlpToolkit</id>
            <url>http://haydut.isikun.edu.tr:8081/artifactory/NlpToolkit</url>
        </repository>
    </distributionManagement>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
                <executions>
                    <execution>
                        <id>attach-javadocs</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-assembly-plugin</artifactId>
                <executions>
                    <execution>
                        <id>sentence-morphological-analysis</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.Sentence.TestSentenceMorphologicalAnalyzerFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>sentence-morphological-analysis</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>sentence-ner</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.Sentence.TestSentenceNERFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>sentence-ner</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>sentence-arguments</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.Sentence.TestSentencePropbankArgumentFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>sentence-arguments</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>sentence-predicate</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.Sentence.TestSentencePropbankPredicateFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>sentence-predicate</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>sentence-semantics</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.Sentence.TestSentenceSemanticFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>sentence-semantics</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>sentence-shallow-parse</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.Sentence.TestSentenceShallowParseFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>sentence-shallow-parse</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-english-semantics</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestEnglishSemanticFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-english-semantics</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-metamorpheme-mover</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestMetaMorphemeMoverFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-metamorpheme-mover</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-morphological-analysis</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestMorphologicalAnalyzerFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-morphological-analysis</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-ner</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestNERFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-ner</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-arguments</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestPropbankArgumentFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-arguments</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-predicate</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestPropbankPredicateFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-predicate</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-translation</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestTranslatorFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-translation</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>tree-turkish-semantics</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.ParseTree.TestTurkishSemanticFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>tree-turkish-semantics</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>wordnet-editor</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.WordNet.TestWordNetEditorFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>wordnet-editor</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>dictionary-editor</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>DataCollector.WordNet.TestDictionaryEditorFrame</mainClass>
                                </manifest>
                            </archive>
                            <finalName>dictionary-editor</finalName>
                        </configuration>
                    </execution>
                </executions>
                <configuration>
                    <descriptorRefs>
                        <descriptorRef>jar-with-dependencies</descriptorRef>
                    </descriptorRefs>
                    <appendAssemblyId>false</appendAssemblyId>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>